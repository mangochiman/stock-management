<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <title>Mahara Wipha Bar</title>
  <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="description" content="">
  <meta name="author" content="">

  <link rel="stylesheet" type="text/css" href="/AircraftAdmin-Free/lib/bootstrap/css/bootstrap.css">
  <link rel="stylesheet" href="/AircraftAdmin-Free/lib/font-awesome/css/font-awesome.css">

  <script src="/AircraftAdmin-Free/lib/jquery-1.11.1.min.js" type="text/javascript"></script>

  <script src="/AircraftAdmin-Free/lib/jQuery-Knob/js/jquery.knob.js" type="text/javascript"></script>

  <script src="/AircraftAdmin-Free/lib/bootstrap-datepicker.js" type="text/javascript"></script>

  <link rel="stylesheet" type="text/css" href="/AircraftAdmin-Free/stylesheets/theme.css">
  <link rel="stylesheet" type="text/css" href="/AircraftAdmin-Free/stylesheets/premium.css">

</head>
<body class=" theme-blue">


<div class="content">

  <div class="main-content">

    <table class="table">
      <thead>
      <tr>
        <th>Name</th>
        <th>Part Number</th>
        <th>Product Label</th>
        <th>Starting inventory</th>
        <th>Stock (In)</th>
        <th>Stock (Out)</th>
        <th>Current stock</th>
        <th>Minimum required</th>
      </tr>
      </thead>
      <tbody>
      <% @products.each do |product| %>
        <tr>
          <td><%= product.name %></td>
          <td><%= product.part_number %></td>
          <td><%= product.label %></td>
          <td><%= product.starting_inventory %></td>
          <td>&nbsp;<%= product.total_stock_ins %></td>
          <td>&nbsp;<%= product.total_stock_outs %></td>
          <td><%= product.current_stock %></td>
          <td><%= product.minimum_required %></td>
        </tr>
      <% end %>
      </tbody>
    </table>

  </div>
</div>

</body>
</html>